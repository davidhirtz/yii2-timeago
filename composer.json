{
  "name": "davidhirtz/yii2-timeago",
  "description": "Displays relative timestamps (e.g. \"4 minutes ago\" or \"about 1 day ago\") via a custom web component.",
  "keywords": [
    "yii2",
    "extension",
    "timeago",
    "time",
    "microtime"
  ],
  "authors": [
    {
      "name": "David Hirtz",
      "email": "hello@davidhirtz.com"
    }
  ],
  "require": {
    "npm-asset/x-timeago": "^1.0",
    "yiisoft/yii2": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\timeago\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
